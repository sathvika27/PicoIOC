record(longout, "$(P):A$(AXIS):MOVE_REL"){
    field(DTYP, "stream")
    field(OUT, "@NewFocus8742.proto move_relative($(P):A$(AXIS):AXIS) $(PORT)")
}
record(longin, "$(P):A$(AXIS):AXIS"){
    field(PINI, "YES")
    field(VAL, "$(AXIS)")
}
record(longin, "$(P):A$(AXIS):GET_VELOCITY"){
    field(PINI, "YES")
    field(DTYP, "stream")
    field(INP, "@NewFocus8742.proto getVelo($(P):A$(AXIS):AXIS) $(PORT)")
}
record(longout, "$(P):A$(AXIS):SET_VELOCITY"){
##    field(PINI, "YES")
    field(DTYP, "stream")
    field(OUT, "@NewFocus8742.proto setVelo($(P):A$(AXIS):AXIS) $(PORT)")
    field(FLNK, $(P):A$(AXIS):GET_VELOCITY)
}
record(longin, "$(P):A$(AXIS):GETPOS"){
    field(DTYP, "stream")
    field(INP, "@NewFocus8742.proto getPos($(P):A$(AXIS):AXIS) $(PORT)")
}

record(seq, "SEQ"){
 #   field(PINI, "YES")
    field(SELM, "All")
    field(LNK1, "$(P):A1:MOVE_REL PP")
    field(DOL1, "200")
    field(DLY1, "10")
    field(LNK2, "$(P):A4:MOVE_REL PP")
    field(DOL2, "200")
    field(DLY2, "10")
    field(FLNK, "SEQ")
}
record(longout, "$(P):$(AXIS):stopMotion"){
    field(DTYP, "stream")
    field(OUT, "@NewFocus8742.proto stopMotion($(P):A$(AXIS):AXIS) $(PORT)")
}
record(bi, "STOP:$(AXIS)"){
   field(FLNK, "$(P):$(AXIS):stopMotion")
}
